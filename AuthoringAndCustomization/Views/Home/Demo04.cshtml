@model AuthoringAndCustomization.Services.EmbeddedReportViewModel;

<div>
  <div id="embed-content-box">
    <div id="embed-container"></div>
  </div>
</div>


<div id="saveas-dialog" class="modal" tabindex="-1">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Save Report As</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <div class="mb-3">
          <label for="report-name" class="form-label">Report Name</label>
          <input type="text" class="form-control" id="report-name" placeholder="report name here" >
        </div>
      </div>
      <div class="modal-footer">
        <button id="save-report" type="button" class="btn btn-primary" disabled  data-bs-dismiss="modal" >Save to Source</button>
        <button id="save-report-to-target" type="button" class="btn btn-primary" disabled  data-bs-dismiss="modal" >Save To Target</button>
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal" >Cancel</button>
      </div>
    </div>
  </div>
</div>


@section Scripts {
<script>
  var viewModel = {
    reportId: "@Model.ReportId",
    embedUrl: "@Model.EmbedUrl",
    token: "@Model.Token",
    targetWorkspaceId: "@Model.TargetWorkspaceId"
  };
</script>
<script src="~/js/app04.js"></script>
}